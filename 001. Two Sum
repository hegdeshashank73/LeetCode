class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        out=[]
        sum1=0
        b=len(nums)
        for i in range(b-1):
            for j in range(i+1,b):
                if(nums[i]+nums[j]==target):
                    out.append(i)
                    out.append(j)
                    return out
